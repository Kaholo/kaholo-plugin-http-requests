{
  "name": "HTTP-Requests",
  "viewName": "HTTP Requests",
  "type": "executer",
  "exposeRoute": false,
  "execProgram": "node",
  "main": "app.js",
  "version": "2.0.0",
  "description": "Send HTTP Requests",
  "imgUrl": "logo.png",
  "category": "Basic Tools",
  "keywords": [
    "http",
    "web",
    "https",
    "api",
    "get",
    "put",
    "curl",
    "post",
    "patch",
    "delete",
    "request",
    "url"
  ],
  "methods": [
    {
      "name": "sendRequest",
      "viewName": "Send HTTP Request",
      "params": [
        {
          "name": "method",
          "viewName": "HTTP Request Method",
          "type": "options",
          "default": "GET",
          "options": [
            {
              "id": "GET",
              "name": "GET"
            },
            {
              "id": "POST",
              "name": "POST"
            },
            {
              "id": "PUT",
              "name": "PUT"
            },
            {
              "id": "PATCH",
              "name": "PATCH"
            },
            {
              "id": "DELETE",
              "name": "DELETE"
            }
          ]
        },
        {
          "name": "url",
          "viewName": "URL",
          "type": "string"
        },
        {
          "name": "body",
          "viewName": "Body",
          "type": "text"
        },
        {
          "name": "headers",
          "viewName": "Headers (Object)",
          "parserType": "object",
          "type": "text"
        },
        {
          "name": "username",
          "viewName": "Username",
          "type": "string"
        },
        {
          "name": "password",
          "viewName": "Password",
          "type": "vault"
        },
        {
          "name": "bearerToken",
          "viewName": "Bearer Token",
          "type": "vault"
        }
      ]
    }
  ]
}